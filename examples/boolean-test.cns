Story: Test Boolean Comparisons

Given:
  x: Integer = 5
  y: Integer = 10
  isValid: Boolean = TRUE
  isInvalid: Boolean = FALSE

Step 1 → If x < y
  Then: Display "x is less than y"
  Otherwise: Display "x is NOT less than y"

Step 2 → If isValid = TRUE
  Then: Display "isValid is TRUE"
  Otherwise: Display "isValid is NOT TRUE"

Step 3 → If isInvalid = FALSE
  Then: Display "isInvalid is FALSE"
  Otherwise: Display "isInvalid is NOT FALSE"

Step 4 → If isValid ≠ isInvalid
  Then: Display "isValid and isInvalid are different"
  Otherwise: Display "isValid and isInvalid are the same"

Step 5 → If isValid = isValid
  Then: Display "isValid equals itself"
  Otherwise: Display "isValid does NOT equal itself"

End: Return 0
